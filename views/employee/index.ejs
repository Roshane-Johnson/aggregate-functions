<table>
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Salary</th>
		<th>Pay Day</th>
		<th>Bonus</th>
		<th>Action</th>
	</tr>
	<tbody>
		<% employees.forEach(employee => { %>
		<tr>
			<td><%= employee.first_nm %></td>
			<td><%= employee.last_nm %></td>
			<td><%= employee.salary %></td>
			<td><%= employee.pay_day %></td>
			<td><%= employee.bonus %></td>
			<td>
				<a href="/employee/edit/<%=employee.id%>">Edit</a>
				<a href="/employee/delete/<%=employee.id%>">Delete</a>
			</td>
		</tr>
		<% }) %>
	</tbody>
</table>
<label for="">Total Salary: <%=aggregate[0].Total_salary%></span></label><span><br>
<label for="">No. of Employees: <%=aggregate[0].Total_Employees%></span></label><span><br>
<label for="">Salary Average: <%=aggregate[0].average_salary%></span></label><span><br>
<label for="">Minimum Salary: <%=aggregate[0].lowest_salary%></span></label><span><br>
<label for="">Maximum Salary: <%=aggregate[0].highest_salary%></span></label><span>


